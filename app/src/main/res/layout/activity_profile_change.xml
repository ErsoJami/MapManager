<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/grey">

    <ImageView
        android:id="@+id/leaveChangingProfileButton"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:clickable="true"
        android:elevation="1dp"
        android:translationZ="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cross" />

    <ImageView
        android:id="@+id/ProfileIconChangingButton"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_photo" />

    <View
        android:id="@+id/changeNicknameView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ProfileIconChangingButton" />

    <View
        android:id="@+id/changeRealNameView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changeNicknameView" />

    <View
        android:id="@+id/changePhoneNumberView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changeRealNameView" />

    <View
        android:id="@+id/changeBirthDateView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changePhoneNumberView" />

    <View
        android:id="@+id/changeEmailView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changeBirthDateView" />

    <View
        android:id="@+id/changeCountryView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changeEmailView" />

    <View
        android:id="@+id/changeCityView"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changeCountryView" />

    <TextView
        android:id="@+id/changeNickText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Никнейм:"
        app:layout_constraintBottom_toBottomOf="@+id/changeNicknameView"
        app:layout_constraintStart_toStartOf="@+id/changeNicknameView"
        app:layout_constraintTop_toTopOf="@+id/changeNicknameView" />

    <TextView
        android:id="@+id/changeRealNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Полное имя:"
        app:layout_constraintBottom_toBottomOf="@+id/changeRealNameView"
        app:layout_constraintStart_toStartOf="@+id/changeRealNameView"
        app:layout_constraintTop_toTopOf="@+id/changeRealNameView" />

    <TextView
        android:id="@+id/changeBirthDateText"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:text="Дата рождения:"
        app:layout_constraintBottom_toBottomOf="@+id/changeBirthDateView"
        app:layout_constraintStart_toStartOf="@+id/changeBirthDateView"
        app:layout_constraintTop_toTopOf="@+id/changeBirthDateView" />

    <TextView
        android:id="@+id/changePhoneNumberText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Телефон:"
        app:layout_constraintBottom_toBottomOf="@+id/changePhoneNumberView"
        app:layout_constraintStart_toStartOf="@+id/changePhoneNumberView"
        app:layout_constraintTop_toTopOf="@+id/changePhoneNumberView" />

    <TextView
        android:id="@+id/changeEmailText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Почта:"
        app:layout_constraintBottom_toBottomOf="@+id/changeEmailView"
        app:layout_constraintStart_toStartOf="@+id/changeEmailView"
        app:layout_constraintTop_toTopOf="@+id/changeEmailView" />

    <TextView
        android:id="@+id/changeCountryText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Страна:"
        app:layout_constraintStart_toStartOf="@+id/changeCountryView"
        app:layout_constraintBottom_toBottomOf="@+id/changeCountryView"
        app:layout_constraintTop_toTopOf="@+id/changeCountryView" />

    <TextView
        android:id="@+id/changeCityText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Город:"
        app:layout_constraintBottom_toBottomOf="@+id/changeCityView"
        app:layout_constraintStart_toStartOf="@+id/changeCityView"
        app:layout_constraintTop_toTopOf="@+id/changeCityView" />

    <TextView
        android:id="@+id/dateText"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="@+id/changeBirthDateView"
        app:layout_constraintStart_toEndOf="@+id/changeBirthDateText"
        app:layout_constraintTop_toTopOf="@+id/changeBirthDateView" />

    <ImageView
        android:id="@+id/dateChangeIcon"
        android:layout_width="18dp"
        android:layout_height="21dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/changeBirthDateView"
        app:layout_constraintEnd_toEndOf="@+id/changeBirthDateView"
        app:layout_constraintTop_toTopOf="@+id/changeBirthDateView"
        app:srcCompat="@drawable/date_icon" />

    <EditText
        android:id="@+id/nickInput"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="100dp"
        android:backgroundTint="@color/material_dynamic_neutral50"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral50"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/changeNicknameView"
        app:layout_constraintStart_toStartOf="@+id/changeNicknameView"
        app:layout_constraintTop_toTopOf="@+id/changeNicknameView" />

    <EditText
        android:id="@+id/realNameInput"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="100dp"
        android:backgroundTint="@color/material_dynamic_neutral50"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral50"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/changeRealNameView"
        app:layout_constraintStart_toStartOf="@+id/changeRealNameView"
        app:layout_constraintTop_toTopOf="@+id/changeRealNameView" />

    <EditText
        android:id="@+id/phoneInput"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="100dp"
        android:backgroundTint="@color/material_dynamic_neutral50"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral50"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/changePhoneNumberView"
        app:layout_constraintStart_toStartOf="@+id/changePhoneNumberView"
        app:layout_constraintTop_toTopOf="@+id/changePhoneNumberView" />

    <EditText
        android:id="@+id/emailInput"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="100dp"
        android:backgroundTint="@color/material_dynamic_neutral50"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral50"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/changeEmailView"
        app:layout_constraintStart_toStartOf="@+id/changeEmailView"
        app:layout_constraintTop_toTopOf="@+id/changeEmailView" />

    <EditText
        android:id="@+id/countryInput"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="100dp"
        android:backgroundTint="@color/material_dynamic_neutral50"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral50"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/changeCountryView"
        app:layout_constraintStart_toStartOf="@+id/changeCountryView"
        app:layout_constraintTop_toTopOf="@+id/changeCountryView" />

    <EditText
        android:id="@+id/cityInput"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="100dp"
        android:backgroundTint="@color/material_dynamic_neutral50"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral50"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/changeCityView"
        app:layout_constraintStart_toStartOf="@+id/changeCityView"
        app:layout_constraintTop_toTopOf="@+id/changeCityView" />
</androidx.constraintlayout.widget.ConstraintLayout>